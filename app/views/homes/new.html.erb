 </p>
 <%end%>






<h> Listing  Details </h>
<div class="media">
<div class="media-body">
	<table class="table table-striped">
		<thead>
			<tr>
				<th>FirstName</th>
				<th>LastName</th>
				<th>Phone</th>
				<th>Email</th>
				<th>Zip</th>
				
			</tr>
		</thead>
		<tbody>
			<%@csv.each do |u|%>
			<tr>
				<td><%= u.FirstName%></td>
				<td><%= u.LastName%></td>
				<td><%=u.Phone%></td>
				<td><%= u.Email%></td>
				<td><%=u.Zip%></td>
				<td><%= button_to "Create Event" , events_path , method: :get , :class => "btn btn-danger"%></td>
			</tr>
			<%end%>
		</tbody>
	</table>
</div>
</div>


